<?xml version="1.0" encoding="utf-8"?>
<moviemasher>


	<control pattern='Clear Video Workspace' 
				tie="player.mash" id='saver'
				url="/music/clear" 
				media="1" mash="1" 
				symbol="/libraries/moviemasher/com/moviemasher/control/Player/stable.swf@CGI" 
				icon="/libraries/moviemasher/com/moviemasher/skin/Liquid/stable.swf@BtnOff"
				overicon="/libraries/moviemasher/com/moviemasher/skin/Liquid/stable.swf@BtnOn"
				disicon="/libraries/moviemasher/com/moviemasher/skin/Liquid/stable.swf@BtnDis"
				fill='stretch' width='155' x="500" y="200" textalign='center' textsize="11" textcolor='333333'
	/>
				
	<control width='4' symbol='/libraries/moviemasher/com/moviemasher/control/Player/stable.swf@Icon' />
	
	<control pattern='Clear Audio Workspace' 
		tie="player.mash" id='saver'
