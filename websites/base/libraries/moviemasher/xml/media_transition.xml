<?xml version="1.0" encoding="UTF-8"?>
<moviemasher>

	<!-- CROSSFADE TRANSITION -->
	<media group='transition' type='transition' id='crossfadetransition' label='Crossfade' 
		symbol='/libraries/moviemasher/com/moviemasher/module/All/stable.swf@Crossfade' 
		icon='/libraries/moviemasher/com/moviemasher/module/All/stable.swf@com.moviemasher.module.Crossfade' 
	/>


